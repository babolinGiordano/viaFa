{
  "name": "viafa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "start:mock": "json-server --watch db.json --port 3000",
    "lint": "ng lint",
    "format": "prettier --write \"{src,projects}/**/*.{ts,html,scss,css,md,json}\"",
    "format:check": "prettier --check \"{src,projects}/**/*.{ts,html,scss,css,md,json}\""
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.2.4",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jest": "^30.0.0",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-preset-angular": "^15.0.0",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.40.0",
    "zone.js": "^0.15.1"
  }
}
